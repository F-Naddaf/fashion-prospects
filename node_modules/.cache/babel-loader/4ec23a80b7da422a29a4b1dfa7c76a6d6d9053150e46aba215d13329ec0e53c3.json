\n        return {\r\n          id: product.id,\r\n          category: product.category,\r\n          title: product.title.slice(0, 35),\r\n          image: product.image,\r\n          price: product.price,\r\n          rate: Math.ceil(product.rating.rate),\r\n        };\r\n      });\r\n      this.products = data;\r\n      this.isLoading = false;\r\n    } catch (error) {\r\n      this.isLoading = false;\r\n      this.error = { status: true, msg: 'Oops something went wrong!' };\r\n    }\r\n  },\r\n  components: {\r\n    LoadingSpinner,\r\n    ProductCard,\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.category-title {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin: 20px 0;\r\n}\r\n.category-title h3 {\r\n  text-transform: capitalize;\r\n  font-size: 22px;\r\n  color: #01689c;\r\n}\r\n.category-title span {\r\n  width: 50px;\r\n  height: 3px;\r\n  border-radius: 5px;\r\n  margin: -18px 0 15px 0;\r\n  background-color: #ff0084;\r\n}\r\nsection {\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n  justify-content: flex-start;\r\n  margin-left: 4%;\r\n}\r\nul {\r\n  display: flex;\r\n  width: 16%;\r\n  height: 300px;\r\n  margin: 2% 3%;\r\n  padding: 1%;\r\n  background: white;\r\n  align-items: flex-end;\r\n  justify-content: center;\r\n  overflow: hidden;\r\n  box-shadow: rgb(99 99 99 / 20%) 0px 2px 8px 0px;\r\n}\r\nul:hover {\r\n  transition-timing-function: ease-in-out;\r\n  transition: 0.5s;\r\n  transform: translateY(-15px);\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}